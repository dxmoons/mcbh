on right-click:
	target block is a smithing table:
		cancel event
		send "&e<!>&c There is a custom recipe for Netherite items."

every 15 minutes:
	set {_rng} to a random number between 0 and 100
	set {_rng} to rounded {_rng}
	if {_rng} is between 0 and 50:
		broadcast "&e<!> &aAll Containers at &c'Bloodgem Mines' &ahas been refilled. &8(near -450 80 700)"
		loop blocks in radius {@radius} around {dg1}:
			if loop-block is a chest:
				clear loop-block's inventory
				loop 8 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
				loop 2 times:
					set {_randomitem} to random element out of {single.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
			if loop-block is a barrel:
				clear loop-block's inventory
				loop 13 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
	if {_rng} is between 51 and 100:
		broadcast "&e<!> &aAll Containers at &c'Whitefoster Den' &ahas been refilled. &8(near -650 60 400)"
		loop blocks in radius {@radius} around {dg2}:
			if loop-block is a chest:
				clear loop-block's inventory
				loop 8 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
				loop 2 times:
					set {_randomitem} to random element out of {single.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
			if loop-block is a barrel:
				clear loop-block's inventory
				loop 13 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory

command /refill:
	permission: op
	trigger:
		loop blocks in radius {@radius} around {dg1}:
			if loop-block is a chest:
				clear loop-block's inventory
				loop 8 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
				loop 2 times:
					set {_randomitem} to random element out of {single.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
			if loop-block is a barrel:
				clear loop-block's inventory
				loop 13 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
		loop blocks in radius {@radius} around {dg2}:
			if loop-block is a chest:
				clear loop-block's inventory
				loop 8 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
				loop 2 times:
					set {_randomitem} to random element out of {single.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
			if loop-block is a barrel:
				clear loop-block's inventory
				loop 13 times:
					set {_randomitem} to random element out of {material.dg1.chest::*}
					add {_randomitem} to loop-block's inventory
					send "&aChest refilled"
command /dg1spot:
	permission: op
	trigger:
		set {dg1} to location of player
		send "&aMiddle set!"
command /dg2spot:
	permission: op
	trigger:
		set {dg2} to location of player
		send "&aMiddle set!"


#crossb : multishot, unbreaking, chance to enroot victim V
#boots: protection, soul walk, speed 1 passive, durability decays V
#sword: sharpness, fagile, chance to lifesteal, chance to blind victim V
#mask: poison heals you, chance to repair your held item, on damage taken chance to gain resistance V
#bloomong set: gives you +5 damage with wooden sword 

#birch single: chance to double output from wood, repair itself, increased protection vs swords
#birch set: +10% succ colleting wood, +5 hearts, negate enroot

#shears(?): sharpness, silk touch, mending, chance to enroot victim, chance to blind victim. 




